<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativelayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.1"
        android:background="@drawable/flower"
        android:src="@drawable/flower" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="85dp"
        android:layout_gravity="center"
        android:layout_marginHorizontal="6dp"
        android:layout_marginVertical="10dp"
        android:text="HOME"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="50sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/currentListings"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginHorizontal="2dp"
        android:layout_marginVertical="60dp"
        android:background="@android:color/transparent"
        android:text="Listings"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"


        />

    <Button
        android:id="@+id/viewMessages"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginHorizontal="80dp"
        android:layout_marginVertical="180dp"
        android:background="@android:color/transparent"
        android:text="View Messages"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"


        />

    <Button
        android:id="@+id/upcomingCleanings"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginHorizontal="100dp"
        android:layout_marginVertical="280dp"
        android:background="@android:color/transparent"
        android:text="Upcoming Cleanings"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"


        />

    <Button
        android:id="@+id/billsReceipts"
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginHorizontal="120dp"
        android:layout_marginVertical="380dp"
        android:background="@android:color/transparent"
        android:text="Billings and Receipts"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"


        />

    ]>
</RelativeLayout>